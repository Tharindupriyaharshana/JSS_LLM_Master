        "                top_idx = np.argsort(-sims)[:TOP_K_CHUNKS]\n",
        "                retrieved_chunks = [chunks[i] for i in top_idx]\n",
        "\n",
        "                user_prompt = build_user_prompt(\n",
        "                    num_jobs=num_jobs,\n",
        "                    num_machines=num_machines,\n",
        "                    retrieved_chunks=retrieved_chunks,\n",
        "                    question_id=q_id,\n",
        "                    question_text=q_text,\n",
        "                )\n",
        "\n",
        "                answers = []\n",
        "                confs = []\n",
        "\n",
        "                for sample_idx in range(NUM_SAMPLES):\n",
        "                    raw_output = safe_call_model(lambda: call_model(user_prompt))\n",
        "                    answer_str, p_true = parse_answer_and_confidence(raw_output)\n",
        "                    answers.append(answer_str)\n",
        "                    confs.append(p_true)\n",
        "                    print(\n",
        "                        f\"    Sample {sample_idx+1}/{NUM_SAMPLES}: \"\n",
        "                        f\"solution {sol_id}, question {q_id}, area {area_num}, conf={p_true}\"\n",
        "                    )\n",
        "\n",
        "                # Build one-row DataFrame for this (solution, question)\n",
        "                row = {\n",
        "                    \"solution_id\": sol_id,\n",
        "                    \"question_id\": q_id,\n",
        "                    \"area_num\": area_num,\n",
        "                }\n",
        "                for i, (a, c) in enumerate(zip(answers, confs), start=1):\n",
        "                    row[f\"answer_{i}\"] = a\n",
        "                    row[f\"p_true_{i}\"] = c\n",
        "\n",
        "                df_row = pd.DataFrame([row])\n",
        "\n",
        "                # Append immediately to CSV\n",
        "                write_header = not os.path.exists(OUTPUT_CSV)\n",
        "                df_row.to_csv(\n",
        "                    OUTPUT_CSV,\n",
        "                    mode=\"a\",\n",
        "                    header=write_header,\n",
        "                    index=False,\n",
        "                )\n",
        "\n",
        "                # Mark this pair as completed (so within the same run we don't redo it)\n",
        "                completed_pairs.add(pair_key)\n",
        "\n",
        "                print(f\"    ✅ Saved result for solution {sol_id}, question {q_id} to {OUTPUT_CSV}\")\n",
        "\n",
        "    except KeyboardInterrupt:\n",
        "        print(\"\\n⏹️ Interrupted by user. All completed pairs so far have been saved.\")\n",
        "    except Exception as e:\n",
        "        print(\"\\n❌ Fatal error in main(). Already-completed pairs are saved.\")\n",
        "        print(e)\n",
        "        raise\n",
        "\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
