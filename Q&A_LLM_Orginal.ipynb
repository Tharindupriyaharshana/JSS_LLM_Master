        "import os\n",
        "import re\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from openai import OpenAI\n",
        "\n",
        "# -----------------------------\n",
        "# Config\n",
        "# -----------------------------\n",
        "MODEL_NAME   = \"gpt-4o\"                 # validator model\n",
        "EMBED_MODEL  = \"text-embedding-3-small\"\n",
        "\n",
        "SOLUTIONS_CSV = \"/content/solutions.csv\"\n",
        "QUESTIONS_CSV = \"/content/20qa_area.csv.csv\"\n",
        "\n",
        "# This should be the multi-answer file from your previous script,\n",
        "# with columns like: solution_id, question_id, answer_1..answer_5, p_true_1..p_true_5\n",
        "ANSWERS_CSV   = \"/content/jss_qa_multi_results.csv\"\n",
        "\n",
        "OUTPUT_CSV    = \"jss_qa_multi_validated.csv\"\n",
        "\n",
        "# RAG hyper-params\n",
        "CHARS_PER_CHUNK = 1200\n",
        "CHUNK_OVERLAP   = 200\n",
        "TOP_K_CHUNKS    = 3\n",
        "\n",
        "# -----------------------------\n",
        "# OpenAI client\n",
        "# -----------------------------\n",
        "# Make sure OPENAI_API_KEY is set in your environment\n",
        "\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Rate-limit / error safe wrapper\n",
        "# -----------------------------\n",
        "def safe_call_model(fn, max_retries=8):\n",
        "    \"\"\"\n",
        "    Calls a function that interacts with OpenAI API safely.\n",
        "    - Retries on rate limits with exponential backoff.\n",
        "    - Fails fast on permission / auth errors (403).\n",
        "    \"\"\"\n",
        "    import random\n",
        "    import time\n",
        "\n",
        "    for attempt in range(max_retries):\n",
        "        try:\n",
        "            return fn()\n",
        "\n",
        "        except Exception as e:\n",
        "            msg = str(e).lower()\n",
        "\n",
        "            # rate limit handling\n",
        "            if \"rate limit\" in msg or \"tpm\" in msg or \"requests per min\" in msg:\n",
        "                wait = 2 ** attempt + random.uniform(0, 1)\n",
        "                print(f\"⚠️ Rate limit hit. Retrying in {wait:.2f} seconds...\")\n",
        "                time.sleep(wait)\n",
        "                continue\n",
        "\n",
        "            # permission / auth / 403 problems – DO NOT retry\n",
        "            if \"permissiondeniederror\" in msg or \"permission denied\" in msg or \"error code: 403\" in msg:\n",
        "                print(\"❌ Permission / auth error from OpenAI API:\")\n",
        "                print(e)\n",
        "                raise\n",
        "\n",
        "            # anything else – just raise\n",
        "            print(\"❌ Unexpected error when calling the model:\")\n",
        "            print(e)\n",
        "            raise\n",
        "\n",
        "    raise RuntimeError(\"❌ Repeated rate limit failures after retries.\")\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Embeddings helpers\n",
        "# -----------------------------\n",
        "def get_embedding(text: str) -> np.ndarray:\n",
        "    resp = client.embeddings.create(\n",
        "        model=EMBED_MODEL,\n",
        "        input=text\n",
        "    )\n",
        "    return np.array(resp.data[0].embedding, dtype=\"float32\")\n",
        "\n",
        "\n",
        "def cosine_sim(a: np.ndarray, b: np.ndarray) -> np.ndarray:\n",
        "    \"\"\"\n",
        "    a: (d,)\n",
        "    b: (n, d)\n",
        "    returns: (n,) cosine similarity\n",
        "    \"\"\"\n",
        "    a_norm = a / (np.linalg.norm(a) + 1e-8)\n",
        "    b_norm = b / (np.linalg.norm(b, axis=1, keepdims=True) + 1e-8)\n",
        "    return b_norm @ a_norm\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Text chunking\n",
        "# -----------------------------\n",
        "def chunk_text(text: str, max_chars: int, overlap: int):\n",
        "    \"\"\"\n",
        "    Split long string into overlapping character chunks.\n",
        "    \"\"\"\n",
        "    text = text.strip()\n",
        "    if len(text) <= max_chars:\n",
        "        return [text]\n",
        "\n",
        "    chunks = []\n",
        "    start = 0\n",
        "    while start < len(text):\n",
        "        end = start + max_chars\n",
        "        chunk = text[start:end]\n",
        "        chunks.append(chunk.strip())\n",
        "        if end >= len(text):\n",
        "            break\n",
        "        start = end - overlap\n",
        "\n",
        "    return chunks\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Build RAG index per solution\n",
        "# -----------------------------\n",
        "def build_rag_index_for_solution(solution_row):\n",
        "    \"\"\"\n",
        "    full solution_text + full tool_assignment into chunks + embeddings.\n",
        "    \"\"\"\n",
        "    solution_text = str(solution_row[\"solution_text\"])\n",
        "    tool_text = str(solution_row[\"tool_assignment\"])\n",
        "\n",
        "    full_doc = f\"SOLUTION:\\n{solution_text}\\n\\nTOOL_ASSIGNMENT:\\n{tool_text}\"\n",
        "    chunks = chunk_text(full_doc, max_chars=CHARS_PER_CHUNK, overlap=CHUNK_OVERLAP)\n",
        "\n",
        "    embeddings = []\n",
        "    for ch in chunks:\n",
        "        emb = safe_call_model(lambda: get_embedding(ch))\n",
        "        embeddings.append(emb)\n",
        "\n",
        "    embeddings = np.vstack(embeddings)\n",
        "    return chunks, embeddings\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Validator prompt builder\n",
        "# -----------------------------\n",
        "def build_validator_prompt(num_jobs,\n",
        "                           num_machines,\n",
        "                           retrieved_chunks,\n",
        "                           question_text,\n",
        "                           original_answer,\n",
        "                           original_p_true):\n",
        "    context_block = \"\\n\\n---\\n\\n\".join(retrieved_chunks)\n",
        "\n",
        "    prompt = f\"\"\"\n",
        "You are validating an answer to a Job-Shop Scheduling (JSS) question.\n",
        "\n",
        "Problem metadata:\n",
        "- Number of jobs: {num_jobs}\n",
        "- Number of machines: {num_machines}\n",
        "\n",
        "The following CONTEXT comes from the full schedule and tool-assignment\n",
        "for one candidate solution (may be split into chunks):\n",
        "\n",
        "{context_block}\n",
        "\n",
        "The ORIGINAL QUESTION was:\n",
        "{question_text}\n",
        "\n",
        "The MODEL'S ANSWER was:\n",
        "{original_answer}\n",
        "\n",
        "The MODEL'S SELF-REPORTED CONFIDENCE (0–100) was:\n",
        "{original_p_true}\n",
        "\n",
        "Your task:\n",
        "- Carefully check whether this ANSWER is actually correct,\n",
        "  based SOLELY on the given schedule context.\n",
        "- Do NOT re-interpret the question; use the same question.\n",
        "- If the answer is fully correct and consistent with the schedule, say it is CORRECT.\n",
        "- Otherwise, say it is INCORRECT.\n",
        "\n",
        "Output format (STRICT):\n",
        "\n",
        "Verdict: CORRECT or INCORRECT\n",
        "Explanation: <very short explanation of why>\n",
        "Confidence: <number between 0 and 100 indicating your confidence in your verdict>\n",
        "\"\"\"\n",
        "    return prompt.strip()\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Call model + parse validator output\n",
        "# -----------------------------\n",
        "def call_validator(user_prompt: str) -> str:\n",
        "    response = client.chat.completions.create(\n",
        "        model=MODEL_NAME,\n",
        "        temperature=0.0,\n",
        "        messages=[\n",
        "            {\n",
        "                \"role\": \"system\",\n",
        "                \"content\": (\n",
        "                    \"You are a strict validator for Job-Shop Scheduling (JSS) answers. \"\n",
        "                    \"Always follow the requested output format carefully.\"\n",
        "                ),\n",
        "            },\n",
        "            {\"role\": \"user\", \"content\": user_prompt},\n",
        "        ],\n",
        "    )\n",
        "    return response.choices[0].message.content\n",
        "\n",
        "\n",
        "def parse_validator_output(full_text: str):\n",
        "    \"\"\"\n",
        "    Parse:\n",
        "      Verdict: CORRECT/INCORRECT\n",
        "      Explanation: ...\n",
        "      Confidence: 0–100\n",
        "    \"\"\"\n",
        "    # Verdict\n",
        "    verdict_match = re.search(r\"Verdict:\\s*(CORRECT|INCORRECT)\", full_text, re.IGNORECASE)\n",
        "    verdict = None\n",
        "    if verdict_match:\n",
        "        verdict = verdict_match.group(1).upper()\n",
        "\n",
        "    # Explanation (everything after 'Explanation:' until 'Confidence:' or end)\n",
        "    expl_match = re.search(\n",
        "        r\"Explanation:\\s*(.*?)(?:\\nConfidence:|\\r\\nConfidence:|\\Z)\",\n",
        "        full_text,\n",
        "        flags=re.DOTALL | re.IGNORECASE,\n",
        "    )\n",
        "    if expl_match:\n",
        "        explanation = expl_match.group(1).strip()\n",
        "    else:\n",
        "        explanation = full_text.strip()\n",
        "\n",
        "    # Confidence 0–100\n",
        "    conf_match = re.search(r\"Confidence:\\s*([0-9]+(?:\\.\\d+)?)\", full_text)\n",
        "    v_conf = None\n",
        "    if conf_match:\n",
        "        try:\n",
        "            v_conf = float(conf_match.group(1))\n",
        "        except ValueError:\n",
        "            v_conf = None\n",
        "\n",
        "    return verdict, explanation, v_conf\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Load completed (solution, question) pairs\n",
        "# -----------------------------\n",
        "def load_completed_pairs(output_csv):\n",
        "    \"\"\"\n",
        "    Returns a set of (solution_id_str, question_id_str) pairs\n",
        "    that already exist in the output CSV.\n",
        "    If the file doesn't exist yet, returns an empty set.\n",
        "    \"\"\"\n",
        "    if not os.path.exists(output_csv):\n",
        "        return set()\n",
        "\n",
        "    df = pd.read_csv(output_csv)\n",
        "    if \"solution_id\" not in df.columns or \"question_id\" not in df.columns:\n",
        "        return set()\n",
        "\n",
        "    sol_ids = df[\"solution_id\"].astype(str)\n",
        "    q_ids = df[\"question_id\"].astype(str)\n",
        "    return set(zip(sol_ids, q_ids))\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Main pipeline\n",
        "# -----------------------------\n",
        "def main():\n",
        "    # Read data\n",
        "    solutions_df = pd.read_csv(SOLUTIONS_CSV, encoding=\"latin1\")\n",
        "    questions_df = pd.read_csv(QUESTIONS_CSV, encoding=\"latin1\")\n",
        "    answers_df   = pd.read_csv(ANSWERS_CSV,   encoding=\"latin1\")\n",
        "\n",
        "    # answers_df = answers_df.sample(n=20, random_state=42)\n",
        "    solutions_df = solutions_df.head(5)\n",
        "\n",
        "    # Detect question cols\n",
        "    possible_qid_cols   = [\"question_id\", \"Question_ID\", \"ID\", \"id\"]\n",
        "    possible_qtext_cols = [\"question_text\", \"Question\", \"question\", \"Text\", \"text\"]\n",
        "\n",
        "    q_id_col = next((c for c in possible_qid_cols if c in questions_df.columns), None)\n",
        "    q_text_col = next((c for c in possible_qtext_cols if c in questions_df.columns), None)\n",
        "\n",
        "    if q_id_col is None or q_text_col is None:\n",
        "        raise ValueError(\n",
        "            \"Could not detect question id/text columns.\\n\"\n",
        "            f\"questions.csv columns: {list(questions_df.columns)}\"\n",
        "        )\n",
        "\n",
        "    # Required columns in solutions\n",
        "    required_solution_cols = {\n",
        "        \"solution_id\",\n",
        "        \"solution_text\",\n",
        "        \"num_jobs\",\n",
        "        \"num_machines\",\n",
        "        \"tool_assignment\",\n",
        "    }\n",
        "    if not required_solution_cols.issubset(solutions_df.columns):\n",
        "        raise ValueError(\n",
        "            f\"solutions.csv must contain columns: {required_solution_cols}, \"\n",
        "            f\"but has: {list(solutions_df.columns)}\"\n",
        "        )\n",
        "\n",
        "    # answers_df must at least have solution_id & question_id\n",
        "    required_answer_cols = {\"solution_id\", \"question_id\"}\n",
        "    if not required_answer_cols.issubset(answers_df.columns):\n",
        "        raise ValueError(\n",
        "            f\"answers CSV must contain columns: {required_answer_cols}, \"\n",
        "            f\"but has: {list(answers_df.columns)}\"\n",
        "        )\n",
        "\n",
        "    # detect answer_i and p_true_i columns\n",
        "    answer_cols = sorted(\n",
        "        [c for c in answers_df.columns if c.startswith(\"answer_\")],\n",
        "        key=lambda x: int(x.split(\"_\")[1])\n",
        "    )\n",
        "\n",
        "    ptrue_cols = sorted(\n",
        "        [c for c in answers_df.columns if c.startswith(\"p_true_\")],\n",
        "        key=lambda x: int(x.split(\"_\")[-1])  # handles p_true_1, p_true_2, etc.\n",
        "    )\n",
        "\n",
        "    if len(answer_cols) == 0:\n",
        "        raise ValueError(\n",
        "            \"No answer_i columns detected in answers CSV. \"\n",
        "            \"Expected columns like 'answer_1', 'answer_2', ...\"\n",
        "        )\n",
        "\n",
        "    if len(ptrue_cols) != len(answer_cols):\n",
        "        print(\"Warning: number of p_true_i columns does not match number of answer_i columns.\")\n",
        "        print(\"answer_cols:\", answer_cols)\n",
        "        print(\"ptrue_cols:\", ptrue_cols)\n",
        "\n",
        "    num_samples = len(answer_cols)\n",
        "    print(f\"Detected {num_samples} answers per (solution, question).\")\n",
        "\n",
        "    # Build lookup maps\n",
        "    solutions_map = {row[\"solution_id\"]: row for _, row in solutions_df.iterrows()}\n",
        "    questions_map = {row[q_id_col]: row[q_text_col] for _, row in questions_df.iterrows()}\n",
        "\n",
        "    # Load already-completed validated pairs (for resume)\n",
        "    completed_pairs = load_completed_pairs(OUTPUT_CSV)\n",
        "    if completed_pairs:\n",
        "        print(f\"Found {len(completed_pairs)} validated (solution, question) pairs in {OUTPUT_CSV}.\")\n",
        "\n",
        "    # 1) Build RAG indexes for all solutions\n",
        "    rag_indexes = {}\n",
        "    print(\"Building RAG indexes for solutions...\")\n",
        "    for sol_id, sol_row in solutions_map.items():\n",
        "        print(f\"  Indexing solution {sol_id}...\")\n",
        "        chunks, embs = build_rag_index_for_solution(sol_row)\n",
        "        rag_indexes[sol_id] = {\n",
        "            \"chunks\": chunks,\n",
        "            \"embeddings\": embs,\n",
        "            \"num_jobs\": int(sol_row[\"num_jobs\"]),\n",
        "            \"num_machines\": int(sol_row[\"num_machines\"]),\n",
        "        }\n",
        "\n",
        "    # 2) Validate each answer_i, saving after each (solution, question) row\n",
        "    print(\"Validating multi-sample answers...\")\n",
        "\n",
        "    try:\n",
        "        for _, ans_row in answers_df.iterrows():\n",
        "            sol_id = ans_row[\"solution_id\"]\n",
        "            q_id   = ans_row[\"question_id\"]\n",
        "            pair_key = (str(sol_id), str(q_id))\n",
        "\n",
        "            # Skip if already validated in OUTPUT_CSV\n",
        "            if pair_key in completed_pairs:\n",
        "                print(f\"Skipping already validated pair: solution {sol_id}, question {q_id}\")\n",
        "                continue\n",
        "\n",
        "            if sol_id not in rag_indexes:\n",
        "                print(f\"Warning: solution_id {sol_id} missing in solutions_df, skipping\")\n",
        "                continue\n",
        "            if q_id not in questions_map:\n",
        "                print(f\"Warning: question_id {q_id} missing in questions_df, skipping\")\n",
        "                continue\n",
        "\n",
        "            idx = rag_indexes[sol_id]\n",
        "            chunks = idx[\"chunks\"]\n",
        "            embs   = idx[\"embeddings\"]\n",
        "            num_jobs = idx[\"num_jobs\"]\n",
        "            num_machines = idx[\"num_machines\"]\n",
        "            question_text = questions_map[q_id]\n",
        "\n",
        "            print(f\"\\n=== Validating solution {sol_id}, question {q_id} ===\")\n",
        "\n",
        "            row_out = {\n",
        "                \"solution_id\": sol_id,\n",
        "                \"question_id\": q_id,\n",
        "            }\n",
        "\n",
        "            # validate each answer_i\n",
        "            for i, ans_col in enumerate(answer_cols, start=1):\n",
        "                p_col = ptrue_cols[i-1] if i-1 < len(ptrue_cols) else None\n",
        "\n",
        "                original_answer = ans_row[ans_col]\n",
        "                original_p_true = ans_row[p_col] if p_col is not None and p_col in ans_row else None\n",
        "\n",
        "                # skip empty answers\n",
        "                if pd.isna(original_answer) or str(original_answer).strip() == \"\":\n",
        "                    row_out[f\"verdict_{i}\"] = None\n",
        "                    row_out[f\"validator_confidence_{i}\"] = None\n",
        "                    row_out[f\"validator_explanation_{i}\"] = None\n",
        "                    continue\n",
        "\n",
        "                # RAG retrieval per answer (uses question + answer)\n",
        "                retrieval_query = f\"QUESTION: {question_text}\\nANSWER: {original_answer}\"\n",
        "                q_emb = safe_call_model(lambda: get_embedding(retrieval_query))\n",
        "                sims = cosine_sim(q_emb, embs)\n",
        "                top_idx = np.argsort(-sims)[:TOP_K_CHUNKS]\n",
        "                retrieved_chunks = [chunks[j] for j in top_idx]\n",
        "\n",
        "                # build prompt\n",
        "                validator_prompt = build_validator_prompt(\n",
        "                    num_jobs=num_jobs,\n",
        "                    num_machines=num_machines,\n",
        "                    retrieved_chunks=retrieved_chunks,\n",
        "                    question_text=question_text,\n",
        "                    original_answer=original_answer,\n",
        "                    original_p_true=original_p_true,\n",
        "                )\n",
        "\n",
        "                raw_output = safe_call_model(lambda: call_validator(validator_prompt))\n",
        "                verdict, explanation, v_conf = parse_validator_output(raw_output)\n",
        "\n",
        "                row_out[f\"verdict_{i}\"] = verdict\n",
        "                row_out[f\"validator_confidence_{i}\"] = v_conf\n",
        "                row_out[f\"validator_explanation_{i}\"] = explanation\n",
        "\n",
        "                print(f\"  Validated answer_{i}: verdict={verdict}, conf={v_conf}\")\n",
        "\n",
        "            # write this (solution, question) row immediately\n",
        "            df_row = pd.DataFrame([row_out])\n",
        "            write_header = not os.path.exists(OUTPUT_CSV)\n",
        "            df_row.to_csv(\n",
        "                OUTPUT_CSV,\n",
        "                mode=\"a\",\n",
        "                header=write_header,\n",
        "                index=False,\n",
        "            )\n",
        "            completed_pairs.add(pair_key)\n",
        "            print(f\"✅ Saved validation row for solution {sol_id}, question {q_id} to {OUTPUT_CSV}\")\n",
        "\n",
        "    except KeyboardInterrupt:\n",
        "        print(\"\\n⏹️ Interrupted by user. All completed validations so far have been saved.\")\n",
        "    except Exception as e:\n",
        "        print(\"\\n❌ Fatal error in main(). Already-completed validations are saved.\")\n",
        "        print(e)\n",
        "        raise\n",
        "\n",
        "    print(f\"\\nDone. Validated results are in {OUTPUT_CSV}\")\n",
        "\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
      ],
