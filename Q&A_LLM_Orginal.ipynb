      "source": [
        "import os\n",
        "import re\n",
        "import time\n",
        "import random\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from openai import OpenAI\n",
        "\n",
        "# -----------------------------\n",
        "# Config\n",
        "# -----------------------------\n",
        "MODEL_NAME = \"gpt-4o\"\n",
        "EMBED_MODEL = \"text-embedding-3-small\"  # or another embedding model\n",
        "\n",
        "SOLUTIONS_CSV = \"/content/solutions.csv\"\n",
        "QUESTIONS_CSV = \"/content/20qa_area.csv\"\n",
        "OUTPUT_CSV = \"jss_qa_multi_results.csv\"\n",
        "\n",
        "# how many answers per (solution, question)\n",
        "NUM_SAMPLES = 5   # 5 answers (1 + 4 more). Change to 4 if you only want 4.\n",
        "\n",
        "# RAG hyper-params\n",
        "CHARS_PER_CHUNK = 1200      # how big each text chunk is (no compression, just splitting)\n",
        "CHUNK_OVERLAP = 200         # overlap for better context\n",
        "TOP_K_CHUNKS = 3            # how many chunks to retrieve per question\n",
        "\n",
        "# -----------------------------\n",
        "# OpenAI client\n",
        "# -----------------------------\n",
        "\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Rate-limit / error safe wrapper\n",
        "# -----------------------------\n",
        "def safe_call_model(fn, max_retries=8):\n",
        "    \"\"\"\n",
        "    Calls a function that interacts with OpenAI API safely.\n",
        "    - Retries on rate limits with exponential backoff.\n",
        "    - Fails fast on permission / auth errors.\n",
        "    \"\"\"\n",
        "    for attempt in range(max_retries):\n",
        "        try:\n",
        "            return fn()\n",
        "\n",
        "        except Exception as e:\n",
        "            msg = str(e).lower()\n",
        "\n",
        "            # rate limit handling\n",
        "            if \"rate limit\" in msg or \"tpm\" in msg or \"requests per min\" in msg:\n",
        "                wait = 2 ** attempt + random.uniform(0, 1)\n",
        "                print(f\"⚠️ Rate limit hit. Retrying in {wait:.2f} seconds...\")\n",
        "                time.sleep(wait)\n",
        "                continue\n",
        "\n",
        "            # permission / auth / 403 problems – DO NOT retry\n",
        "            if \"permissiondeniederror\" in msg or \"permission denied\" in msg or \"error code: 403\" in msg:\n",
        "                print(\"❌ Permission / auth error from OpenAI API:\")\n",
        "                print(e)\n",
        "                raise\n",
        "\n",
        "            # anything else – just raise\n",
        "            print(\"❌ Unexpected error when calling the model:\")\n",
        "            print(e)\n",
        "            raise\n",
        "\n",
        "    raise RuntimeError(\"❌ Repeated rate limit failures after retries.\")\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Embeddings helpers\n",
        "# -----------------------------\n",
        "def get_embedding(text: str) -> np.ndarray:\n",
        "    resp = client.embeddings.create(\n",
        "        model=EMBED_MODEL,\n",
        "        input=text\n",
        "    )\n",
        "    return np.array(resp.data[0].embedding, dtype=\"float32\")\n",
        "\n",
        "\n",
        "def cosine_sim(a: np.ndarray, b: np.ndarray) -> np.ndarray:\n",
        "    \"\"\"\n",
        "    a: (d,)\n",
        "    b: (n, d)\n",
        "    returns: (n,) cosine similarity\n",
        "    \"\"\"\n",
        "    a_norm = a / (np.linalg.norm(a) + 1e-8)\n",
        "    b_norm = b / (np.linalg.norm(b, axis=1, keepdims=True) + 1e-8)\n",
        "    return b_norm @ a_norm\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Text chunking (no compression, full strings)\n",
        "# -----------------------------\n",
        "def chunk_text(text: str, max_chars: int, overlap: int):\n",
        "    \"\"\"\n",
        "    Split a long string into overlapping chunks by characters.\n",
        "    Keeps the raw text 'as is' – just slices.\n",
        "    \"\"\"\n",
        "    text = text.strip()\n",
        "    if len(text) <= max_chars:\n",
        "        return [text]\n",
        "\n",
        "    chunks = []\n",
        "    start = 0\n",
        "    while start < len(text):\n",
        "        end = start + max_chars\n",
        "        chunk = text[start:end]\n",
        "        chunks.append(chunk.strip())\n",
        "        if end >= len(text):\n",
        "            break\n",
        "        start = end - overlap  # move window with overlap\n",
        "\n",
        "    return chunks\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Build RAG index per solution\n",
        "# -----------------------------\n",
        "def build_rag_index_for_solution(solution_row):\n",
        "    \"\"\"\n",
        "    For a single solution, build:\n",
        "      - chunks: list[str]\n",
        "      - embeddings: np.ndarray shape (n_chunks, dim)\n",
        "    using full solution_text + full tool_assignment (no compression).\n",
        "    \"\"\"\n",
        "    solution_text = str(solution_row[\"solution_text\"])\n",
        "    tool_text = str(solution_row[\"tool_assignment\"])\n",
        "\n",
        "    full_doc = f\"SOLUTION:\\n{solution_text}\\n\\nTOOL_ASSIGNMENT:\\n{tool_text}\"\n",
        "    chunks = chunk_text(full_doc, max_chars=CHARS_PER_CHUNK, overlap=CHUNK_OVERLAP)\n",
        "\n",
        "    embeddings = []\n",
        "    for ch in chunks:\n",
        "        emb = safe_call_model(lambda: get_embedding(ch))\n",
        "        embeddings.append(emb)\n",
        "\n",
        "    embeddings = np.vstack(embeddings)  # (n_chunks, d)\n",
        "    return chunks, embeddings\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Prompt builder (uses retrieved chunks)\n",
        "# -----------------------------\n",
        "def build_user_prompt(num_jobs, num_machines, retrieved_chunks, question_id, question_text):\n",
        "    context_block = \"\\n\\n---\\n\\n\".join(retrieved_chunks)\n",
        "\n",
        "    prompt = f\"\"\"\n",
        "You are analyzing a Job-Shop Scheduling (JSS) solution.\n",
        "\n",
        "Problem metadata:\n",
        "- Number of jobs: {num_jobs}\n",
        "- Number of machines: {num_machines}\n",
        "\n",
        "The following CONTEXT comes from the full schedule and tool-assignment\n",
        "for a single candidate solution. It may be split into several chunks:\n",
        "\n",
        "{context_block}\n",
        "\n",
        "Now answer the following question about THIS schedule:\n",
        "\n",
        "Question ID {question_id}: {question_text}\n",
        "\n",
        "First, provide your reasoning for the answer.\n",
        "Then give the final answer in the format:\n",
        "Answer: <your final answer>\n",
        "\n",
        "After you finish the final answer, on a NEW LINE, output a confidence, how sure abot the answere score from 0 to 1\n",
        "representing the probability (can be 0. something) that your answer is correct.\n",
        "\n",
        "Format exactly:\n",
        "Confidence: <number between 0 and 1>\n",
        "\"\"\"\n",
        "    return prompt.strip()\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Call model + parse output\n",
        "# -----------------------------\n",
        "def call_model(user_prompt: str) -> str:\n",
        "    response = client.chat.completions.create(\n",
        "        model=MODEL_NAME,\n",
        "        temperature=0.7,  # >0 to get variation across samples\n",
        "        messages=[\n",
        "            {\n",
        "                \"role\": \"system\",\n",
        "                \"content\": (\n",
        "                    \"You are an expert in Job-Shop Scheduling (JSS). \"\n",
        "                    \"Always follow the requested output format carefully.\"\n",
        "                ),\n",
        "            },\n",
        "            {\"role\": \"user\", \"content\": user_prompt},\n",
        "        ],\n",
        "    )\n",
        "    return response.choices[0].message.content\n",
        "\n",
        "\n",
        "def parse_answer_and_confidence(full_text: str):\n",
        "    conf_match = re.search(r\"Confidence:\\s*([0-9]+(?:\\.\\d+)?)\", full_text)\n",
        "    p_true = None\n",
        "    if conf_match:\n",
        "        try:\n",
        "            p_true = float(conf_match.group(1))\n",
        "        except ValueError:\n",
        "            p_true = None\n",
        "\n",
        "    ans_match = re.search(\n",
        "        r\"Answer:\\s*(.*?)(?:\\nConfidence:|\\r\\nConfidence:|\\Z)\",\n",
        "        full_text,\n",
        "        flags=re.DOTALL | re.IGNORECASE,\n",
        "    )\n",
        "    if ans_match:\n",
        "        answer_str = ans_match.group(1).strip()\n",
        "    else:\n",
        "        answer_str = full_text.strip()\n",
        "\n",
        "    return answer_str, p_true\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Load completed (solution, question) pairs\n",
        "# -----------------------------\n",
        "def load_completed_pairs(output_csv):\n",
        "    \"\"\"\n",
        "    Returns a set of (solution_id_str, question_id_str) pairs\n",
        "    that already exist in the output CSV.\n",
        "    If the file doesn't exist yet, returns an empty set.\n",
        "    \"\"\"\n",
        "    if not os.path.exists(output_csv):\n",
        "        return set()\n",
        "\n",
        "    df = pd.read_csv(output_csv)\n",
        "    if \"solution_id\" not in df.columns or \"question_id\" not in df.columns:\n",
        "        return set()\n",
        "\n",
        "    sol_ids = df[\"solution_id\"].astype(str)\n",
        "    q_ids = df[\"question_id\"].astype(str)\n",
        "    return set(zip(sol_ids, q_ids))\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Main pipeline\n",
        "# -----------------------------\n",
        "def main():\n",
        "    solutions_df = pd.read_csv(SOLUTIONS_CSV)\n",
        "    questions_df = pd.read_csv(QUESTIONS_CSV)\n",
        "\n",
        "    # OPTIONAL: limit for testing\n",
        "    solutions_df = solutions_df.head(1)\n",
        "    questions_df = questions_df.head(5)\n",
        "\n",
        "    required_solution_cols = {\n",
        "        \"solution_id\",\n",
        "        \"solution_text\",\n",
        "        \"num_jobs\",\n",
        "        \"num_machines\",\n",
        "        \"tool_assignment\",\n",
        "    }\n",
        "    if not required_solution_cols.issubset(solutions_df.columns):\n",
        "        raise ValueError(f\"solutions.csv must contain columns: {required_solution_cols}\")\n",
        "\n",
        "    # also require area_num in questions\n",
        "    required_question_cols = {\"question_id\", \"question_text\", \"area_num\"}\n",
        "    if not required_question_cols.issubset(questions_df.columns):\n",
        "        raise ValueError(\n",
        "            f\"questions.csv must contain columns: {required_question_cols}, \"\n",
        "            f\"but has: {list(questions_df.columns)}\"\n",
        "        )\n",
        "\n",
        "    # Load already-completed pairs (for resume)\n",
        "    completed_pairs = load_completed_pairs(OUTPUT_CSV)\n",
        "    if completed_pairs:\n",
        "        print(f\"Found {len(completed_pairs)} completed (solution, question) pairs in {OUTPUT_CSV}.\")\n",
        "\n",
        "    # Build RAG indexes for all solutions\n",
        "    rag_indexes = {}  # solution_id -> {chunks, embeddings, num_jobs, num_machines}\n",
        "    print(\"Building RAG indexes for solutions...\")\n",
        "    for _, sol_row in solutions_df.iterrows():\n",
        "        sol_id = sol_row[\"solution_id\"]\n",
        "        print(f\"  Indexing solution {sol_id}...\")\n",
        "        chunks, embs = build_rag_index_for_solution(sol_row)\n",
        "        rag_indexes[sol_id] = {\n",
        "            \"chunks\": chunks,\n",
        "            \"embeddings\": embs,\n",
        "            \"num_jobs\": int(sol_row[\"num_jobs\"]),\n",
        "            \"num_machines\": int(sol_row[\"num_machines\"]),\n",
        "        }\n",
        "\n",
        "    print(\"Running multi-sample Q&A over solutions...\")\n",
        "\n",
        "    try:\n",
        "        for _, sol_row in solutions_df.iterrows():\n",
        "            sol_id = sol_row[\"solution_id\"]\n",
        "            print(f\"\\n=== Processing solution {sol_id} ===\")\n",
        "\n",
        "            idx = rag_indexes[sol_id]\n",
        "            chunks = idx[\"chunks\"]\n",
        "            embs = idx[\"embeddings\"]\n",
        "            num_jobs = idx[\"num_jobs\"]\n",
        "            num_machines = idx[\"num_machines\"]\n",
        "\n",
        "            for _, q_row in questions_df.iterrows():\n",
        "                q_id = q_row[\"question_id\"]\n",
        "                pair_key = (str(sol_id), str(q_id))\n",
        "\n",
        "                # Skip if we already have this pair in OUTPUT_CSV\n",
        "                if pair_key in completed_pairs:\n",
        "                    print(f\"  Skipping already completed pair: solution {sol_id}, question {q_id}\")\n",
        "                    continue\n",
        "\n",
        "                q_text = q_row[\"question_text\"]\n",
        "                area_num = q_row[\"area_num\"]\n",
        "\n",
        "                print(f\"  -> New pair: solution {sol_id}, question {q_id}, area {area_num}\")\n",
        "\n",
        "                # Retrieve context ONCE for this (solution, question)\n",
        "                q_emb = safe_call_model(lambda: get_embedding(str(q_text)))\n",
