        "import os\n",
        "import re\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from openai import OpenAI\n",
        "\n",
        "# -----------------------------\n",
        "# Config\n",
        "# -----------------------------\n",
        "MODEL_NAME = \"gpt-4o\"\n",
        "EMBED_MODEL = \"text-embedding-3-small\"  # or another embedding model\n",
        "SOLUTIONS_CSV = \"/content/solutions.csv\"\n",
        "QUESTIONS_CSV = \"/content/20qa_area.csv\"\n",
        "OUTPUT_CSV = \"jss_qa_results_final.csv\"\n",
        "\n",
        "\n",
        "# RAG hyper-params\n",
        "CHARS_PER_CHUNK = 1200\n",
        "CHUNK_OVERLAP = 200\n",
        "TOP_K_CHUNKS = 3\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Embeddings helpers\n",
        "# -----------------------------\n",
        "def get_embedding(text: str) -> np.ndarray:\n",
        "    resp = client.embeddings.create(\n",
        "        model=EMBED_MODEL,\n",
        "        input=text\n",
        "    )\n",
        "    return np.array(resp.data[0].embedding, dtype=\"float32\")\n",
        "\n",
        "\n",
        "def cosine_sim(a: np.ndarray, b: np.ndarray) -> np.ndarray:\n",
        "    a_norm = a / (np.linalg.norm(a) + 1e-8)\n",
        "    b_norm = b / (np.linalg.norm(b, axis=1, keepdims=True) + 1e-8)\n",
        "    return b_norm @ a_norm\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Text chunking\n",
        "# -----------------------------\n",
        "def chunk_text(text: str, max_chars: int, overlap: int):\n",
        "    text = text.strip()\n",
        "    if len(text) <= max_chars:\n",
        "        return [text]\n",
        "\n",
        "    chunks = []\n",
        "    start = 0\n",
        "    while start < len(text):\n",
        "        end = start + max_chars\n",
        "        chunk = text[start:end]\n",
        "        chunks.append(chunk.strip())\n",
        "        if end >= len(text):\n",
        "            break\n",
        "        start = end - overlap\n",
        "    return chunks\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Build RAG index per solution\n",
        "# -----------------------------\n",
        "def build_rag_index_for_solution(solution_row):\n",
        "    solution_text = str(solution_row[\"solution_text\"])\n",
        "    tool_text = str(solution_row[\"tool_assignment\"])\n",
        "\n",
        "    full_doc = f\"SOLUTION:\\n{solution_text}\\n\\nTOOL_ASSIGNMENT:\\n{tool_text}\"\n",
        "    chunks = chunk_text(full_doc, max_chars=CHARS_PER_CHUNK, overlap=CHUNK_OVERLAP)\n",
        "\n",
        "    embeddings = []\n",
        "    for ch in chunks:\n",
        "        emb = get_embedding(ch)\n",
        "        embeddings.append(emb)\n",
        "\n",
        "    embeddings = np.vstack(embeddings)  # (n_chunks, d)\n",
        "    return chunks, embeddings\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Prompt builder\n",
        "# -----------------------------\n",
        "def build_user_prompt(num_jobs, num_machines, retrieved_chunks, question_id, question_text):\n",
        "    context_block = \"\\n\\n---\\n\\n\".join(retrieved_chunks)\n",
        "\n",
        "    prompt = f\"\"\"\n",
        "You are analyzing a Job-Shop Scheduling (JSS) solution.\n",
        "\n",
        "Problem metadata:\n",
        "- Number of jobs: {num_jobs}\n",
        "- Number of machines: {num_machines}\n",
        "\n",
        "The following CONTEXT comes from the full schedule and tool-assignment\n",
        "for a single candidate solution. It may be split into several chunks:\n",
        "\n",
        "{context_block}\n",
        "\n",
        "Now answer the following question about THIS schedule:\n",
        "\n",
        "Question ID {question_id}: {question_text}\n",
        "\n",
        "First, provide your reasoning for the answer.\n",
        "Then give the final answer in the format:\n",
        "Answer: <your final answer>\n",
        "\n",
        "After you finish the final answer, on a NEW LINE, output a confidence score , that mean how sure are you from 0 to 1\n",
        "representing the probability ( can be something link 0.2..) that your answer is correct.\n",
        "\n",
        "Format exactly:\n",
        "Confidence: <number between 0 and 100>\n",
        "\"\"\"\n",
        "    return prompt.strip()\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Call model + parse output\n",
        "# -----------------------------\n",
        "def call_model(user_prompt: str) -> str:\n",
        "    response = client.chat.completions.create(\n",
        "        model=MODEL_NAME,\n",
        "        temperature=2,  # deterministic for eval\n",
        "        messages=[\n",
        "            {\n",
        "                \"role\": \"system\",\n",
        "                \"content\": (\n",
        "                    \"You are an expert in Job-Shop Scheduling (JSS). \"\n",
        "                    \"Always follow the requested output format carefully.\"\n",
        "                ),\n",
        "            },\n",
        "            {\"role\": \"user\", \"content\": user_prompt},\n",
        "        ],\n",
        "    )\n",
        "    return response.choices[0].message.content\n",
        "\n",
        "\n",
        "def parse_answer_and_confidence(full_text: str):\n",
        "    conf_match = re.search(r\"Confidence:\\s*([0-9]+(?:\\.\\d+)?)\", full_text)\n",
        "    p_true = None\n",
        "    if conf_match:\n",
        "        try:\n",
        "            p_true = float(conf_match.group(1))\n",
        "        except ValueError:\n",
        "            p_true = None\n",
        "\n",
        "    ans_match = re.search(\n",
        "        r\"Answer:\\s*(.*?)(?:\\nConfidence:|\\r\\nConfidence:|\\Z)\",\n",
        "        full_text,\n",
        "        flags=re.DOTALL | re.IGNORECASE,\n",
        "    )\n",
        "    if ans_match:\n",
        "        answer_str = ans_match.group(1).strip()\n",
        "    else:\n",
        "        answer_str = full_text.strip()\n",
        "\n",
        "    return answer_str, p_true\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Main pipeline\n",
        "# -----------------------------\n",
        "def main():\n",
        "    solutions_df = pd.read_csv(SOLUTIONS_CSV)\n",
        "    questions_df = pd.read_csv(QUESTIONS_CSV)\n",
        "\n",
        "    solutions_df = solutions_df.head(1)\n",
        "    questions_df = questions_df.head(2)\n",
        "\n",
        "    required_solution_cols = {\n",
        "        \"solution_id\",\n",
        "        \"solution_text\",\n",
        "        \"num_jobs\",\n",
        "        \"num_machines\",\n",
        "        \"tool_assignment\",\n",
        "    }\n",
        "    if not required_solution_cols.issubset(solutions_df.columns):\n",
        "        raise ValueError(f\"solutions.csv must contain columns: {required_solution_cols}\")\n",
        "\n",
        "    required_question_cols = {\"question_id\", \"question_text\", \"area_num\"}\n",
        "    if not required_question_cols.issubset(questions_df.columns):\n",
        "        raise ValueError(\n",
        "            f\"questions.csv must contain columns: {required_question_cols}, \"\n",
        "            f\"but has: {list(questions_df.columns)}\"\n",
        "        )\n",
        "\n",
        "    # 1) Build RAG indexes for all solutions\n",
        "    rag_indexes = {}\n",
        "\n",
        "    print(\"Building RAG indexes for solutions...\")\n",
        "    for _, sol_row in solutions_df.iterrows():\n",
        "        sol_id = sol_row[\"solution_id\"]\n",
        "        print(f\"  Indexing solution {sol_id}...\")\n",
        "        chunks, embs = build_rag_index_for_solution(sol_row)\n",
        "        rag_indexes[sol_id] = {\n",
        "            \"chunks\": chunks,\n",
        "            \"embeddings\": embs,\n",
        "            \"num_jobs\": int(sol_row[\"num_jobs\"]),\n",
        "            \"num_machines\": int(sol_row[\"num_machines\"]),\n",
        "        }\n",
        "\n",
        "    # 2) Ask questions using retrieved chunks\n",
        "    print(\"Running Q&A over solutions...\")\n",
        "    results = []\n",
        "\n",
        "    for _, sol_row in solutions_df.iterrows():\n",
        "        sol_id = sol_row[\"solution_id\"]\n",
        "        idx = rag_indexes[sol_id]\n",
        "        chunks = idx[\"chunks\"]\n",
        "        embs = idx[\"embeddings\"]\n",
        "        num_jobs = idx[\"num_jobs\"]\n",
        "        num_machines = idx[\"num_machines\"]\n",
        "\n",
        "        for _, q_row in questions_df.iterrows():\n",
        "            q_id = q_row[\"question_id\"]\n",
        "            q_text = q_row[\"question_text\"]\n",
        "            area_num = q_row[\"area_num\"]\n",
        "\n",
        "            # Embed the question for retrieval\n",
        "            q_emb = get_embedding(str(q_text))\n",
        "            sims = cosine_sim(q_emb, embs)\n",
        "            top_idx = np.argsort(-sims)[:TOP_K_CHUNKS]\n",
        "            retrieved_chunks = [chunks[i] for i in top_idx]\n",
        "\n",
        "            user_prompt = build_user_prompt(\n",
        "                num_jobs=num_jobs,\n",
        "                num_machines=num_machines,\n",
        "                retrieved_chunks=retrieved_chunks,\n",
        "                question_id=q_id,\n",
        "                question_text=q_text,\n",
        "            )\n",
        "\n",
        "            raw_output = call_model(user_prompt)\n",
        "            answer_str, p_true = parse_answer_and_confidence(raw_output)\n",
        "\n",
        "            results.append(\n",
        "                {\n",
        "                    \"solution_id\": sol_id,\n",
        "                    \"question_id\": q_id,\n",
        "                    \"area_num\": area_num,\n",
        "                    \"answer\": answer_str,\n",
        "                    \"p_true\": p_true,\n",
        "                }\n",
        "            )\n",
        "            print(f\"Done: solution {sol_id}, question {q_id}, area {area_num}\")\n",
        "\n",
        "    results_df = pd.DataFrame(results)\n",
        "    results_df.to_csv(OUTPUT_CSV, index=False)\n",
        "    print(f\"Saved results to {OUTPUT_CSV}\")\n",
        "\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
      ]
    },
