        "import time\n",
        "import random\n",
        "from openai import OpenAIError\n",
        "\n",
        "def safe_call_model(fn, max_retries=8):\n",
        "    \"\"\"\n",
        "    Calls a function that interacts with OpenAI API safely.\n",
        "    Automatically retries on rate limits with exponential backoff.\n",
        "    \"\"\"\n",
        "    for attempt in range(max_retries):\n",
        "        try:\n",
        "            return fn()\n",
        "        except Exception as e:\n",
        "            # Catch rate limits only\n",
        "            if \"rate_limit\" in str(e).lower() or \"tpm\" in str(e).lower():\n",
        "                wait = 2 ** attempt + random.uniform(0, 1)\n",
        "                print(f\"⚠️ Rate limit hit. Retrying in {wait:.2f} seconds...\")\n",
        "                time.sleep(wait)\n",
        "                continue\n",
        "            else:\n",
        "                raise  # real error, rethrow\n",
        "    raise RuntimeError(\"❌ Repeated rate limit failures after retries.\")\n"
