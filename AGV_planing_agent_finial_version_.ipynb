        "# ============================================================\n",
        "#  1. Imports and setup\n",
        "# ============================================================\n",
        "from langchain_openai import ChatOpenAI, OpenAIEmbeddings\n",
        "from langchain_community.vectorstores import Chroma\n",
        "from langchain.prompts import ChatPromptTemplate\n",
        "import os, time, pandas as pd, numpy as np\n",
        "\n",
        "# --- Compatibility patches (NumPy + Chroma) -----------------\n",
        "if not hasattr(np, \"float_\"): np.float_ = np.float64\n",
        "if not hasattr(np, \"int_\"): np.int_ = np.int64\n",
        "if not hasattr(np, \"bool_\"): np.bool_ = np.bool_\n",
        "\n",
        "import chromadb\n",
        "if not hasattr(chromadb, \"config\"):\n",
        "    import types\n",
        "    chromadb.config = types.SimpleNamespace(Settings=chromadb.Settings)\n",
        "\n",
        "# ---  Enter your API key ---\n",
        "os.environ[\"OPENAI_API_KEY\"] = \"sd"\n",
        "\n",
        "\n",
        "# ============================================================\n",
        "#  2. Build vector database for AGV Logistics Knowledge\n",
        "# ============================================================\n",
        "os.makedirs(\"vector_db/agv_knowledge\", exist_ok=True)\n",
        "os.makedirs(\"data\", exist_ok=True)\n",
        "\n",
        "with open(\"data/agv_planning_knowledge.txt\") as f:\n",
        "    raw = f.read()\n",
        "\n",
        "chunks = [c.strip() for c in raw.split(\"----- SECTION -----\") if c.strip()]\n",
        "\n",
        "emb = OpenAIEmbeddings(model=\"text-embedding-3-small\")\n",
        "agv_rag = Chroma.from_texts(chunks, embedding=emb, persist_directory=\"./vector_db/agv\")\n",
        "agv_rag.persist()\n",
        "# ============================================================\n",
        "#  3. LLM setup\n",
        "# ============================================================\n",
        "llm = ChatOpenAI(model=\"gpt-4o-mini\", temperature=0)\n",
        "\n",
        "# ============================================================\n",
        "#  4. Prompt templates\n",
        "# ============================================================\n",
        "\n",
        "# --- Baseline / Simple Prompt ---\n",
        "baseline_template = ChatPromptTemplate.from_template(\n",
        "    \"\"\"You are an AGV planning assistant.\n",
        "Given the schedule and tool sharing plan, produce a concise AGV plan:\n",
        "- LAGV: Warehouse -> first machine for each job (finish before Op0 start).\n",
        "- UAGV: Between consecutive operations; and last machine -> Warehouse.\n",
        "- TAGV: As per tool sharing transitions (finish before destination op start).\n",
        "If simultaneous tasks occur for the same AGV type, add another vehicle (e.g., LAGV2).\n",
        "\n",
        "Return only the AGV plan in sections:\n",
        "AGV Transport Plan\n",
        "- LAGV (Material Loading)\n",
        "- UAGV (Inter-Machine Transfers)\n",
        "- TAGV (Tool Transfers)\n",
        "\n",
        "Schedule:\n",
        "{schedule}\n",
        "\n",
        "Tool Sharing Plan:\n",
        "{sharing_plan}\n",
        "\"\"\"\n",
        ")\n",
        "\n",
        "# --- Detailed Prompt (your spec, lightly structured) ---\n",
        "detailed_template = ChatPromptTemplate.from_template(\n",
        "    \"\"\"You are an expert in Flexible Job Shop Scheduling (FJSS) with integrated AGV logistics and tool transport.\n",
        "\n",
        "Given:\n",
        "- A Job Shop Solution (operations with start, duration, end, machine)\n",
        "- A Tool Sharing Plan (which tools move between machines and jobs)\n",
        "\n",
        "Generate a complete AGV Transport Plan covering:\n",
        "1) LAGV  Warehouse -> first machine before Job's first op.\n",
        "2) UAGV  Machine(i) -> Machine(i+1) before next op start; after last op -> Warehouse.\n",
        "3) TAGV  Tool transfers per tool sharing transitions, before destination op start.\n",
        "\n",
        "Rules:\n",
        "- Each AGV handles one task at a time.\n",
        "- If same-type tasks overlap, instantiate more AGVs (LAGV1, LAGV2, ...).\n",
        "- Assume negligible trip time or +1 min buffer.\n",
        "\n",
        "Output Format:\n",
        "AGV Transport Plan\n",
        " LAGV (Material Loading)\n",
        "<LAGV tasks...>\n",
        " UAGV (Inter-Machine Transfers)\n",
        "<UAGV tasks...>\n",
        " TAGV (Tool Transfers)\n",
        "<TAGV tasks...>\n",
        "\n",
        "Schedule:\n",
        "{schedule}\n",
        "\n",
        "Tool Sharing Plan:\n",
        "{sharing_plan}\n",
        "\"\"\"\n",
        ")\n",
        "\n",
        "# --- RAG + Context Prompt (with richer doc) ---\n",
        "rag_template = ChatPromptTemplate.from_template(\n",
        "    \"\"\"You are an expert in FJSS with AGV logistics and tool transport.\n",
        "Use this knowledge:\n",
        "{context}\n",
        "\n",
        "Task:\n",
        "- Build LAGV tasks (Warehouse -> first machines).\n",
        "- Build UAGV tasks (Machine(i) -> Machine(i+1); final -> Warehouse).\n",
        "- Build TAGV tasks from tool sharing transitions.\n",
        "- Finish each move before the target op's start time.\n",
        "- Resolve overlaps by instantiating multiple AGVs of the same type.\n",
        "- Use negligible/+1 buffer if travel time unspecified.\n",
        "- Name vehicles LAGV1..n, UAGV1..n, TAGV1..n.\n",
        "\n",
        "Return only:\n",
        "AGV Transport Plan\n",
        " LAGV (Material Loading)\n",
        "<one task per line>\n",
        " UAGV (Inter-Machine Transfers)\n",
        "<one task per line>\n",
        " TAGV (Tool Transfers)\n",
        "<one task per line>\n",
        "\n",
        "Schedule:\n",
        "{schedule}\n",
        "\n",
        "Tool Sharing Plan:\n",
        "{sharing_plan}\n",
        "\"\"\"\n",
        ")\n",
        "\n",
        "# ============================================================\n",
        "#  5. Run evaluation pipeline (2 samples only)\n",
        "# ============================================================\n",
        "\n",
        "# Uses your previously prepared file with Question + Tool_Sharing_Plan\n",
        "df = pd.read_excel(\"/content/valid_50_with_tool_lifetime.xlsx\")\n",
        "\n",
        "results = []\n",
        "for i, row in enumerate(df.head(2).iterrows(), start=1):  #  only first 2\n",
        "    _, row = row\n",
        "    qid = row[\"ID\"]\n",
        "    schedule = row[\"Question\"]\n",
        "    sharing_plan = row[\"Tool_Sharing_Plan\"] if pd.notna(row[\"Tool_Sharing_Plan\"]) else \"\"\n",
        "\n",
        "    print(f\" Processing AGV plan for ID={qid} ...\")\n",
        "\n",
        "    # --- Baseline ---\n",
        "    base_msgs = baseline_template.format_messages(\n",
        "        schedule=schedule,\n",
        "        sharing_plan=sharing_plan,\n",
        "    )\n",
        "    base_out = llm.invoke(base_msgs).content\n",
        "\n",
        "    # --- Detailed ---\n",
        "    det_msgs = detailed_template.format_messages(\n",
        "        schedule=schedule,\n",
        "        sharing_plan=sharing_plan,\n",
        "    )\n",
        "    det_out = llm.invoke(det_msgs).content\n",
        "\n",
        "    # --- RAG + Context ---\n",
        "    docs = agv_rag.similarity_search(\"AGV Logistics Knowledge Base\", k=2)\n",
        "    context = \"\\n\".join([d.page_content for d in docs]) if docs else \"\"\n",
        "    rag_msgs = rag_template.format_messages(\n",
        "        context=context,\n",
        "        schedule=schedule,\n",
        "        sharing_plan=sharing_plan,\n",
        "    )\n",
        "    rag_out = llm.invoke(rag_msgs).content\n",
        "\n",
        "    results.append(\n",
        "        {\n",
        "            \"ID\": qid,\n",
        "            \"Schedule\": schedule,\n",
        "            \"Tool_Sharing_Plan\": sharing_plan,\n",
        "            \"Baseline_Output\": base_out.strip(),\n",
        "            \"Detailed_Output\": det_out.strip(),\n",
        "            \"RAG_Output\": rag_out.strip(),\n",
        "        }\n",
        "    )\n",
        "    time.sleep(0.5)\n",
        "\n",
        "# ============================================================\n",
        "#  6. Save final results\n",
        "# ============================================================\n",
        "os.makedirs(\"outputs\", exist_ok=True)\n",
        "out_path = \"outputs/agv_planning_comparison.xlsx\"\n",
        "pd.DataFrame(results).to_excel(out_path, index=False)\n",
        "print(f\"\\n Results saved to {out_path}\")\n"
